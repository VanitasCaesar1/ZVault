{
  "name": "@zvault/sdk",
  "version": "0.1.0",
  "description": "Official ZVault SDK for Node.js â€” fetch secrets at runtime from ZVault Cloud",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "zvault",
    "secrets",
    "secrets-manager",
    "env",
    "environment-variables",
    "cloud",
    "sdk"
  ],
  "author": "ZVault <hello@zvault.cloud>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArcadeLabsInc/zvault",
    "directory": "sdks/node"
  },
  "homepage": "https://zvault.cloud",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "@types/node": "^22.0.0"
  }
}
